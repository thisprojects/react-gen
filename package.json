{
  "name": "reactgen",
  "version": "1.0.0",
  "description": "Interactive React component assistant REPL tool",
  "type": "module",
  "bin": {
    "reactgen": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsc",
    "start": "node dist/cli.js",
    "prepare": "npm run build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "keywords": [
    "react",
    "repl",
    "component",
    "cli",
    "assistant"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.28.5",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "fast-glob": "^3.3.2",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/babel__traverse": "^7.20.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
